package teste;

public class product {
	
	String name = "melancia";
	double price = 17.00;
	int quantity = 10;
	
	public double totalvalueinstock() {
		System.out.println("name = " + name);
		System.out.println("price = " + price);
		System.out.println("quantity =" + quantity);
		
		return (quantity * price);
	}

}
