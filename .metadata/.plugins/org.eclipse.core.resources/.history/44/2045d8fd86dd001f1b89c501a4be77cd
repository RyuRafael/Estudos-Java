package teste;

import java.util.Scanner;

public class  Main{

public static void main(String[]args) {
	
	Scanner sc = new Scanner(System.in);
	
	
	int n1 = sc.nextInt();
	int soma = 0;
	
	for(int i = 0; i < n1; i++) {
		
		int n = sc.nextInt();
		soma += n;
	
		}
	
	System.out.println(soma);
	}
}
